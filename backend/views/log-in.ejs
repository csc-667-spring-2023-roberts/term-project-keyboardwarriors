<html>
  <head>
    <%- include('./partials/header', {title: title}); %>
    <link rel="stylesheet" href="/stylesheets/authentication.css" type="text/css">
  </head>
  <body>
    <div id="content-wrap">
      <%- include('./partials/navbar', {state: "log-in"}); %>

      <h1 id="pageTitle">Log-In (Unauthenticated)</h1>

      <div id="formHolder">
        <form action="/authentication/log-in" method="post">
          <label for="email">Email</label>
          <input
            id="email"
            name="email"
            type="email"
            placeholder="Email"
            autocomplete="email"
            required />
          
          <label for="password">Password</label>
          <input
            id="password"
            name="password"
            type="password"
            placeholder="Password"
            autocomplete="current-password"
            required />
          
          <div id="buttonHolder">
            <input type="submit" value="Log In" />
          </div>
        </form>
      </div>
    </div>
    <%- include('./partials/footer'); %>
  </body>
</html>